<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(contact_form_field) -%>">
  <span class='title'>
    <%= contact_form_field.field_name %>

  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_contact_form_fields_admin_contact_form_field_path(contact_form_field),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.contact_form_fields_admin_contact_form_field_path(contact_form_field),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => contact_form_field.field_name),
        :method => :delete %>
  </span>
</li>
